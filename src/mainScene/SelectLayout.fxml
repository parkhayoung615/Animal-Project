<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="selectPane" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainScene.MainController">
   <children>
      <Button fx:id="catSelectBtn" layoutX="125.0" layoutY="600.0" mnemonicParsing="false" onAction="#changeCatScene" prefHeight="40.0" prefWidth="200.0" text="고양이" textFill="WHITE">
         <font>
            <Font name="SeoulHangangC EB" size="24.0" />
         </font>
      </Button>
      <Button fx:id="dogSelectBtn" layoutX="500.0" layoutY="600.0" mnemonicParsing="false" onAction="#changeDogScene" prefHeight="40.0" prefWidth="200.0" text="강아지" textFill="WHITE">
         <font>
            <Font name="SeoulHangangC EB" size="24.0" />
         </font>
      </Button>
      <Button fx:id="multiSelectBtn" layoutX="884.0" layoutY="600.0" mnemonicParsing="false" onAction="#changemultiScene" prefHeight="40.0" prefWidth="200.0" text="강아지 &amp; 고양이" textFill="WHITE">
         <font>
            <Font name="SeoulHangangC EB" size="24.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
