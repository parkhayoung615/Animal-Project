<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="endDogPane" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainScene.MainController">
   <children>
      <Button fx:id="DogHomeBtn" layoutX="350.0" layoutY="440.0" mnemonicParsing="false" onAction="#changeDogHomeScene" prefHeight="50.0" prefWidth="500.0" text="HOME" textFill="WHITE" />
      <Button fx:id="dogCloseBtn" layoutX="350.0" layoutY="525.0" mnemonicParsing="false" onAction="#closeDog" prefHeight="50.0" prefWidth="500.0" text="END" textFill="WHITE" />
   	  <Button fx:id="DogChangeBtn" layoutX="350.0" layoutY="613.0" mnemonicParsing="false" onAction="#changeDogSelectScene" prefHeight="50.0" prefWidth="500.0" text="CHANGE" textFill="WHITE" />
   </children>
</AnchorPane>
